# ReplenMobile Backend - Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Key (for GPT-4o Vision invoice parsing)
OPENAI_API_KEY=your_openai_api_key_here

# Yahoo Japan Shopping API (for barcode lookup)
YAHOO_API_KEY=your_yahoo_api_key_here

# ClickSend API (for fax sending)
CLICKSEND_USERNAME=your_clicksend_username
CLICKSEND_API_KEY=your_clicksend_api_key

# Server configuration
PORT=8000
